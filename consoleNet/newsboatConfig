max-items	0
auto-reload	yes
browser		firefox

macro y		set browser "bash $baseLoc/.newsboat/newsboatInterface.sh down" ; open-in-browser ; set browser firefox 

macro u		set browser "bash $baseLoc/.newsboat/newsboatInterface.sh open" ; open-in-browser ; set browser firefox

macro d		set browser "bash $baseLoc/.newsboat/newsboatInterface.sh delete" ; open-in-browser ; set browser firefox

macro p		set browser "bash $baseLoc/.newsboat/youtubeFeeder.sh" ; open-in-browser ; set browser firefox

macro q		set browser "bash $baseLoc/.newsboat/youtubeFeeder.sh $(cat $baseLoc/.newsboat/RSS/_ytURLS)" ; open-in-browser ; set browser firefox
